<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/libs/bootstrap/css/bootstrap.css">
    <script src="/js/jquery.js"></script>
    <script src="/js/aes.js"></script>

    <script src="/libs/bootstrap/js/bootstrap.js"></script>
    <script src="https://tranquanghuy.herokuapp.com/socket.io/socket.io.js"></script>
    <script src="/js/app.js"></script>
    <script src="/js/scripts.js"></script>

    <title>Document</title>
</head>

<body>


    <!--Header-->
    <div class="header text-center">
        <div class="loginForm">

            <h3>What's your name?</h3>
            
            <div class="form-inline">
                <div class="col-xs-8 col-xs-offset-2">
                    <input style="" class="form-control" id="txtUserName" type="text">
                    <input class="btn btn-info" value="Register" id="btnRegister" type="button">
                    <p class="" id="registerResult"></p>
                    
                </div>


            </div>

        </div>
    </div>


    <!--Container-->
    <div class="wrapper">


        <div class="chatForm">

            <div class="container">

                <nav class="navbar navbar-info bg-info">
                    <div class="container-fluid">
                        <!-- Brand and toggle get grouped for better mobile display -->
                        <div class="navbar-header" >
                           <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="true" style="background-color: #c1c1c1;">
                            <span class="sr-only" style="
                            background-color: #fcf8e3;
                            ">Toggle navigation</span>
                            <span class="icon-bar" style="
                            background-color: #fcf8e3;
                            "></span>
                            <span class="icon-bar" style="
                            background-color: #fcf8e3;
                            "></span>
                            <span class="icon-bar" style="
                            background-color: #fcf8e3;
                            "></span>

                        </button>
                        <a class="navbar-brand" href="#">Chat Room</a>
                    </div>

                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <div id="user">
                            <ul class="nav navbar-nav navbar-right">
                                <li><a href="#" id="btnLogout">Logout</a></li>
                            </ul>
                            <p class="navbar-text navbar-right">Hello, <span id="currentUser"></span><a href="#" class="navbar-link" id="userInfo"></a></p>
                        </div>
                    </div>
                    <!-- /.navbar-collapse -->
                </div>
                <!-- /.container-fluid -->
            </nav>

            <div class="row">
                <div class="col-md-3" id="left">
                    <div class="panel panel-info">
                        <div class="panel-heading">Users Online</div>
                        <div class="panel-body" id="listUsers">
                            Panel content
                        </div>
                    </div>
                </div>
                <div class="col-md-9" id="right">
                    <div class="panel panel-info">
                        <div class="panel-body" id="listMessages">
                        </div>
                        <div><small id="gophim"></small></div>
                        <div class="panel-footer panel-info">
                            <div class="input-group">
                                <input id="txtMessage" type="text" class="form-control" placeholder="Type your message here..." />
                                <span class="input-group-btn">
                                    <button class="btn btn-info" id="btnSendMessage">Send</button>
                                </span>
                                    <!--<span class="input-group-btn">
                                    <button class="btn btn-danger" id="btnNoEncrypt">Send without encryption</button>
                                </span>-->
                            </div>
                        </div>

                    </div>
                    <!--<div class="form-inline">-->


                    <!--</div>-->
                </div>
            </div>
            <!--end of row-->
        </div>
        <!--end of container-->
    </div>



</div>

<!--Footer-->
<div class="footer">

</div>


<!--Scripts-->


</body>

</html>