<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/libs/bootstrap/css/bootstrap.css">
    <script src="/js/jquery.js"></script>
    <script src="/libs/bootstrap/js/bootstrap.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/app.js"></script>
    <script src="/js/chat.js"></script>

    <script src="/js/scripts.js"></script>

    <title>Document</title>
</head>

<body>
    <!--Header-->
    <div class="header">
        <div class="loginForm">
            <h3>What's your name?</h3>
            <input type="text" id="txtUserName">
            <input type="button" value="Register" id="btnRegister">
        </div>
    </div>

    <!--Container-->
    <div class="wrapper">


        <div class="chatForm">
            <div class="sayHi">
                Hello <span id="currentUser"></span>
                <input type="button" id="btnLogout" value="logout">
            </div>
            <div class="container">
                <div class="row">
                    <div class="col-md-4" id="left">
                        <div class="panel panel-default">
                            <div class="panel-heading">Users Online</div>
                            <div class="panel-body" id="listUsers">
                                Panel content
                            </div>
                        </div>
                    </div>
                    <div class="col-md-8" id="right">
                        <div class="panel panel-default">
                            <div class="panel-heading">Panel heading without title</div>
                            <div class="panel-body" id="listMessages">
                                Panel content
                            </div>
                        </div>
                        <input type="text" id="txtMessage" />
                        <input type="button" id="btnSendMessage" value="Send" />
                    </div>
                </div>
                <!--end of row-->
            </div>
            <!--end of container-->
        </div>



    </div>

    <!--Footer-->
    <div class="footer">

    </div>


    <!--Scripts-->


</body>

</html>